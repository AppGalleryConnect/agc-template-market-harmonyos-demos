{
  "app": {
    "signingConfigs": [
    ],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.0(12)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "common",
      "srcPath": "./common"
    },
    {
      "name": "RouteDesign",
      "srcPath": "./feature/RouteDesign",
    },
    {
      "name": "CodeScan",
      "srcPath": "./feature/CodeScan"
    },
    {
      "name": "PersonalCenter",
      "srcPath": "./feature/PersonalCenter",
    },
    {
      "name": "frontpage",
      "srcPath": "./feature/frontpage",
    }
  ]
}