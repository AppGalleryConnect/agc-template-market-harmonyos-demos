{
  "app": {
    "signingConfigs": [],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.0(12)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./products/entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "utils",
      "srcPath": "./commons/utils"
    },
    {
      "name": "city_select",
      "srcPath": "./commons/city_select"
    },
    {
      "name": "address_manage",
      "srcPath": "./commons/address_manage"
    },
    {
      "name": "home",
      "srcPath": "./features/home"
    },
    {
      "name": "category",
      "srcPath": "./features/category"
    },
    {
      "name": "mine",
      "srcPath": "./features/mine"
    },
  ]
}