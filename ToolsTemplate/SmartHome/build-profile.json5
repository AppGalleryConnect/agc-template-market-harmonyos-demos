{
  "app": {
    "signingConfigs": [
    ],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.0(12)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "phone",
      "srcPath": "./products/phone",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "device_mng",
      "srcPath": "./features/device_mng",
    },
    {
      "name": "utils",
      "srcPath": "./commons/utils",
    },
    {
      "name": "device_service",
      "srcPath": "./features/device_service",
    },
    {
      "name": "device_simulation",
      "srcPath": "./test/device_simulation",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "device_add",
      "srcPath": "./features/device_add",
    },
    {
      "name": "external_interactions",
      "srcPath": "./commons/external_interactions",
    },
    {
      "name": "uicomponents",
      "srcPath": "./commons/uicomponents",
    }
  ]
}