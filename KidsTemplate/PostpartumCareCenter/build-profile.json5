{
  "app": {
    "signingConfigs": [
    ],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.1(13)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "phone",
      "srcPath": "./products/phone",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "home",
      "srcPath": "./scenes/home"
    },
    {
      "name": "stores",
      "srcPath": "./scenes/stores",
    },
    {
      "name": "mine",
      "srcPath": "./scenes/mine",
    },
    {
      "name": "uicomponents",
      "srcPath": "./components/uicomponents",
    },
    {
      "name": "utils",
      "srcPath": "./components/utils",
    },
    {
      "name": "activities",
      "srcPath": "./scenes/activities",
    }
  ]
}